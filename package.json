{
  "name": "cuurly-api",
  "version": "1.0.0",
  "author": "hi@ezeikel.dev",
  "license": "ISC",
  "scripts": {
    "bake-env": "node ./scripts/process.env-generator.js",
    "dev": "yarn bake-env && yarn prisma:generate && sls offline --aws-profile ezeikel-admin",
    "lint": "jest --config test/jest.lint.js",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json|css|md|mdx|html)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "deploy": "NODE_ENV=production sls deploy --aws-profile ezeikel-admin",
    "undeploy": "sls remove --aws-profile ezeikel-admin",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prepare": "husky install"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "lint-staged": {
    "src/**/*.+(ts|js|json|md)": [
      "prettier --write",
      "jest --findRelatedTests"
    ]
  },
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "@sentry/serverless": "^7.8.1",
    "apollo-server-lambda": "^3.10.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.30.1",
    "cookie-parser": "^1.4.5",
    "crypto": "^1.0.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-upload": "13.0.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "nodemailer": "^6.7.7",
    "path": "^0.12.7",
    "stripe": "^10.0.0",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-typescript": "7.18.6",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.13",
    "@types/graphql": "14.5.0",
    "@types/graphql-upload": "8.0.11",
    "@types/jsonwebtoken": "8.5.8",
    "@types/md5": "^2.3.2",
    "@types/node": "18.6.3",
    "@types/nodemailer": "6.4.4",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "babel-loader": "8.2.5",
    "cache-loader": "4.1.0",
    "copy-webpack-plugin": "11.0.0",
    "eslint": "8.21.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "is-ci-cli": "2.2.0",
    "jest": "27.5.1",
    "jest-in-case": "1.0.2",
    "jest-runner-eslint": "1.0.1",
    "jest-watch-select-projects": "2.0.0",
    "jest-watch-typeahead": "2.0.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prisma": "^4.1.1",
    "serverless-domain-manager": "6.0.3",
    "serverless-offline": "8.5.0",
    "serverless-offline-variables": "0.1.0",
    "serverless-webpack": "5.8.0",
    "serverless-webpack-prisma": "1.1.0",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-node-externals": "3.0.0"
  }
}
